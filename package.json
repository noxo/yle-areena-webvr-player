{
  "name": "yle-areena-webvr-player",
  "version": "0.1.0",
  "description": "Simple WEBVR client to watch YLE Areena",
  "license": "MIT",
  "author": "Erkki Nokso-Koivisto",
  "main": "build/bundle.js",
  "scripts": {
    "build-client": "browserify src/client/index.js -t [ babelify --presets [ es2015 ] ] --debug | exorcist dist/client.js.map > dist/client.js",
    "build-server": "babel --plugins transform-es2015-modules-commonjs src/server -o dist/server.js",
    "build": "npm run-script build-server & npm run-script build-client",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-server": "^2.1.18",
    "css-loader": "0.23.1",
    "exorcist": "^0.4.0"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "express": "^4.15.2",
    "http-proxy-middleware": "^0.17.4",
    "jquery": "^3.1.1",
    "path": "^0.12.7",
    "request": "^2.81.0",
    "three": "^0.84.0",
    "urijs": "^1.17.0",
    "url-parse": "^1.1.8"
  }
}
